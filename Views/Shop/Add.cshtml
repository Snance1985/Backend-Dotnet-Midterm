@model midterm_project.Models.Shop

@{
    ViewData["Title"] = "New Dino";
}

<div class="resume-wrapper">
    <form asp-controller="Shop" asp-action="Add" method="post">
        <h1 class="display-4 text-center mb-5">Add New Dinos!</h1>

        @await Html.PartialAsync("_ShopInput", Model)

        <input type="submit" class="btn btn-sm btn-primary float-end" value="Save">
    </form>

    <a class="btn btn-sm btn-light" asp-controller="Shop" asp-action="List">Back to all Dinos!</a>
</div>